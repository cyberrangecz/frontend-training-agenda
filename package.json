  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "ci-test": "ng test --browsers ChromeHeadless --watch=false",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "build-example-app": "ng build training-agenda-example-app --configuration production",
    "build-library": "ng build kypo-training-agenda --configuration production",
    "create-package": "cd dist/kypo-training-agenda && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd ./projects/kypo-training-agenda/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-training-agenda && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
